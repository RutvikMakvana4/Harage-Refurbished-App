                <!-- <input type="text" class="form-control" placeholder="Search"> -->
                <!-- </form> -->
                <!-- </div> -->
                <!-- </div> -->
                <!-- </div> -->

                <div class="header-right">
                    <ul class="clearfix">
                        <li class="icons dropdown">
                            <div class="user-img c-pointer position-relative" data-toggle="dropdown">
                                <span class="activity active"></span>
                                <img src="/adminWeb/img/covor.png" height="40" width="40" alt="">
                            </div>
                            <div class="drop-down dropdown-profile animated fadeIn dropdown-menu">
                                <div class="dropdown-content-body">
                                    <ul>
                                        <li><a href="/webAdmin/Logout" onclick="logoutAdmin(event)"><i
                                                    class="fa fa-sign-out" aria-hidden="true"></i>
                                                <span><b>Logout</b></span></a>
                                        </li>
                                        <% if (assignRoles.includes("Administrator") || assignRoles.includes("Product Manager") ||
                                            assignRoles.includes("Acquisition Manager") || assignRoles.includes("Order Manager") || assignRoles.includes("Order Manager") ) {
                                            %>
                                            <li><a href="/webAdmin/change-password">ðŸ”’&nbsp;
