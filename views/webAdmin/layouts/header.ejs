                                                    <li>
                                                        <a href="/webAdmin/order/" class="" aria-expanded="false">
                                                            <i class="fa fa-shopping-bag"></i>
                                                            <span class="nav-text">User Purchased Orders</span>
                                                        </a>
                                                    </li>
                                                    <% } %>
                                                        <% if (assignRoles.includes("Administrator") || assignRoles.includes("Customer Support Specialist") ) { %>
                                                            <li>
                                                                <a href="/webAdmin/return-exchange/" class=""
                                                                    aria-expanded="false">
                                                                    <i class="fa fa-shopping-bag"></i>
                                                                    <span class="nav-text">Return/Exchange</span>
                                                                </a>
                                                            </li>
                                                            <% } %>
                                                           <% if (assignRoles.includes("Administrator") || assignRoles.includes("Customer Support Specialist") ) { %>
                                                               <li>
                                                                   <a href="/webAdmin/report/" class="" aria-expanded="false">
                                                                       <i class="fa fa-shopping-bag"></i>
                                                                       <span class="nav-text">Reported Items</span>
                                                                   </a>
                                                               </li>
                                                               <% } %>
                                                            <% if (assignRoles.includes("Administrator")) { %>
                                                                <li>
                                                                    <a class="has-arrow" href="javascript:void()"
                                                                        aria-expanded="false">
                                                                        <i class="icon-envelope menu-icon"></i> <span
                                                                            class="nav-text">Logs</span>
                                                                    </a>
                                                                    <ul aria-expanded="false">
                                                                      
                                                                            <li>
                                                                                <a href="/webAdmin/managelogs/user/">User
                                                                                    Logs</a>
                                                                            </li>
                                                                                    <li>
                                                                                        <a
                                                                                            href="/webAdmin/managelogs/admin/">Admin
                                                                                            Logs</a>
                                                                                    </li>
                                                                                    <% } %>
                                                                    </ul>
                                                                </li>
            </ul>
