                                    <li>
                                        <a href="/webAdmin/product/product-page/all" class="" aria-expanded="false">
                                            <i class="fa fa-archive"></i>
                                            <span class="nav-text">Manage Products</span>
                                        </a>
                                    </li>
                                    <% } %>
                                        <% if (assignRoles.includes("Administrator") || assignRoles.includes("Acquisition Manager") || assignRoles.includes("Order Manager")) { %>
                                            <li>
                                                <a href="/webAdmin/quick-sell/" class="" aria-expanded="false">
