            <div class="brand-logo">
                <a href="/webAdmin/dashboard" class="p-0">
                    <b class="logo-abbr"><img src="/img/logo/harage.png" alt="" height="50%"> </b>
                    <span class="brand-title">
                        <h2 class="m-0 p-0" style="text-align: center; color: white;">H A R A G E</h2>
                    </span>
                </a>
            </div>
        </div>
        <!--**********************************
            Nav header end
        ***********************************-->

        <!--**********************************
            Header start
        ***********************************-->
        <div class="header position-fixed top-0">
            <div class="header-content clearfix">

                <div class="nav-control">
                    <div class="hamburger">
                        <span class="toggle-icon"><i class="icon-menu"></i></span>
                    </div>
                </div>


                <!-- <div class="header-left"> -->
