    <style>
        .image-item {
            display: inline-block;
            position: relative;
            margin: 5px;
        }

        .close {
            position: absolute;
            top: 0;
            right: 0;
            color: white;
            background-color: black;
            padding: 2px 4px;
            border-radius: 50%;
            cursor: pointer;
        }

        .close:hover {
            background-color: #fff;
        }

        .uploaded-image {
            width: 100px;
            height: 100px;
        }

        .upload__inputfile {
            width: 0.1px;
            height: 0.1px;
            opacity: 0;
            overflow: hidden;
            position: absolute;
            z-index: -1;
        }

        .upload__img-box {
            width: 100%;
            height: fit-content;
            object-fit: cover;
        }

        .upload__img-wrap {
            display: grid;
            grid-template-columns: repeat(6, 1fr);
            gap: 5px;
            /* max-height: 100px; */
            width: 100%;
        }

        .upload__img-close {
            width: 24px;
            height: 24px;
            border-radius: 50%;
            background-color: white;
            position: absolute;
            top: 10%;
            right: 10px;
            cursor: pointer;
        }

        .upload__img-close::after {
            content: "-";
            display: flex;
            justify-content: center;
            align-items: center;
            color: black;
        }

        .img-bg {
            background-repeat: no-repeat;
            background-position: center;
            background-size: cover;
            position: relative;
            /* padding-bottom: 70%; */
            min-height: 100px;
            height: 100%;
            min-width: 100px;
            width: 100%;
            /* margin: 8px 12px -27px 13px; */
            /* border-radius: 10px; */
        }

        .custum-file-upload {
            width: 100%;
            align-items: space-between;
            gap: 10px;
            cursor: pointer;
            padding: 1.5rem;
            /* border-radius: 10px; */
            border: 1px dashed rgba(0, 0, 0, 0.6);
            background: #f6f6f6;
        }

        .custum-file-upload .text span {
            font-weight: 400;
            color: rgba(75, 85, 99, 1);
        }

        .custom-file {
            height: 100%;
        }

        .upload__img-close button {
            transform: translate(-50%, -50%);
            left: 50%;
            position: absolute;
            top: 50%;
            border: none;
            background-color: transparent;
        }

        @media only screen and (min-width: 0px) and (max-width: 576px) {
            .upload__img-wrap {
                grid-template-columns: repeat(2, 1fr) !important;
            }
        }

        @media only screen and (min-width: 576px) and (max-width: 768px) {
            .upload__img-wrap {
                grid-template-columns: repeat(3, 1fr) !important;
            }
        }

        @media only screen and (min-width: 768px) and (max-width: 1270px) {
            .upload__img-wrap {
                grid-template-columns: repeat(4, 1fr) !important;
            }
        }

        @media only screen and (min-width: 991px) and (max-width: 1270px) {
            .upload__img-wrap {
                grid-template-columns: repeat(4, 1fr) !important;
            }
        }

        @media only screen and (min-width: 1270px) and (max-width: 1650px) {
            .upload__img-wrap {
                grid-template-columns: repeat(5, 1fr) !important;
            }
        }

        .input-group-append {
            cursor: pointer;
        }

        .col-5 {
            flex: 0 0 49.99%;
            max-width: 100%
        }

        h4 a {
            color: #000000;
        }

        h4 a:hover {
            color: #000;
            text-decoration: none;
        }

        .card-title {
            display: flex;
            align-items: center;
            justify-content: left;
            margin-left: 1px;
        }

        .hidden-field {
            display: none;
        }

        label.error {
            color: red;
        }
    </style>

