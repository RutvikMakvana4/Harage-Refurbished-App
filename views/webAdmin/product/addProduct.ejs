                        required: "carrier is required."
                    },
                    otherCarrier: {
                        required: "Other carrier is required."
                    },
                    headType: {
                        required: "Head type is required."
                    },
                    otherHeadType: {
                        required: "Other Head Type is required."
                    },
                    size: {
                        required: "Size is required."
                    },
                    otherSize: {
                        required: "Other Size is required."
                    },
                },
                errorPlacement: function (error, element) {
                    if (element.attr("name") == "image") {
                        error.appendTo("#imageError");
                    } else {
                        error.insertAfter(element);
                    }
                },
                submitHandler: function (form) {
                    event.preventDefault();

                    var formData = new FormData(form);

                    var fieldsToCheck = ['productName', 'brand', 'model', 'screenSize', 'screenType', 'resolution', 'condition', 'price', 'color', 'connectivity', 'earCoupling', 'headphoneStyle', 'noiseCancellationLevel', 'includedSubwooder', 'powerOutput', 'channelConfiguration', 'includedSoundbar', 'includedReceiver', 'speakerConfiguration', 'systemType', 'componentIncluded', 'internalStorage', 'microphoneType', 'recordingFormats', 'compatibleHeadphoneModel', 'mountType', 'vesaCompatibility', 'weightCapacity', 'screenSizeRange', 'length', 'hdmiVersion', 'remoteCompatibility', 'type', 'formFactor', 'processor', 'ram', 'storage', 'graphicsCard', 'operatingSystem', 'displayResolution', 'displaySize', 'displayRefreshRate', 'switchType', 'layout', 'sensorType', 'maxDpi', 'numberOfButtons', 'headedness', 'material', 'bagType', 'fitsLaptopSize', 'storageCapacity', 'driveType', 'interface', 'networkCompatibility', 'carrier', 'displayType', 'cameraDetails', 'compatibility', 'bandStyle', 'caseType', 'chargerType', 'outputPower', 'connectorType', 'compatibleDevices', 'accessories', 'discEdition', 'consoleGeneration', 'consoleModel', 'compatiblePlatforms', 'connectionType', 'size', 'gripStyle', 'weight', 'platformCompatibility', 'audioType', 'microphone', 'refrigeratorType', 'totalCapacity', 'washerType', 'capacity', 'acType', 'coolingCapacity', 'vacuumType', 'cordedCordless', 'sliceCapacity', 'blenderType', 'jarCapacity', 'power', 'coffeeType', 'hvacSystemCompatibility', 'deviceType', 'videoResolution', 'suitableFor', 'lockType', 'lightingType', 'shaverType', 'wattage', 'hairDryerType', 'primaryUseCases', 'compatibleWith', 'monitorType', 'display', 'wearStyle', 'sleepTracking', 'measurements', 'compatibleApps', 'maxVideoResolution', 'cameraType', 'sensorSize', 'megaPixels', 'lensMount', 'lensType', 'includedLenses', 'primaryMaterial', 'focalLength', 'maximumAperture', 'tripodType', 'maximumHeight', 'foldedLength', 'loadCapacity', 'headType', 'thickness', 'batteryLife', 'compatiblePhoneModel', 'systemCompatibility', 'handedness', 'appCompatibility', 'warrantyStatus', 'additionalFeatures', 'description'];

                    fieldsToCheck.forEach(function (field) {
                        var fieldElement = $("#" + field);
                        var value = fieldElement.is(':visible') ? fieldElement.val() : null;
                        if (value !== null) {
                            formData.set(field, value);
                        } else {
                            formData.delete(field);
                        }
                    });

                    function setclassDataValue(selector, key, otherSelector) {
                        let select = '';
                        const elements = document.querySelectorAll(selector);

                        elements.forEach(element => {
                            if (element.value !== "") {
                                select = element.value;
                            }
                        });

                        if (select === "Other") {
                            const otherValue = $(otherSelector).val();
                            if (otherValue !== "") {
                                formData.set(key, otherValue);
                            } else {
                                formData.delete(key);
                            }
                        } else if (select !== "") {
                            formData.set(key, select);
                        } else {
                            formData.delete(key);
                        }
                    }

                    setclassDataValue('.brand', 'brand', '#otherBrand');
                    setclassDataValue('.color', 'color', '#otherColor');
                    setclassDataValue('.resolution', 'resolution', '#otherResolution');
                    setclassDataValue('.displayResolution', 'displayResolution', '#otherDisplayResolution');
                    setclassDataValue('.lightingType', 'lightingType', '#otherLightingType');
                    setclassDataValue('.material', 'material', '#otherMaterial');
                    setclassDataValue('.length', 'length', '#otherLength');
                    setclassDataValue('.channelConfiguration', 'channelConfiguration', '#otherChannelConfiguration');
                    setclassDataValue('.speakerConfiguration', 'speakerConfiguration', '#otherSpeakerConfiguration');
                    setclassDataValue('.carrier', 'carrier', '#otherCarrier');
                    setclassDataValue('.headType', 'headType', '#otherHeadType');
                    setclassDataValue('.size', 'size', '#otherSize');
                    setclassDataValue('.screenType', 'screenType', '#otherScreenType');

                    function setCheckboxValue(checkboxId, formData, fieldName) {
                        var checkboxValue = $(checkboxId).is(":checked") ? true : false;
                        formData.set(fieldName, checkboxValue);
                    }

                    setCheckboxValue("#smartTVCheckbox", formData, 'smartTv');
                    setCheckboxValue("#hdrCheckbox", formData, 'hdr');
                    setCheckboxValue("#stereoPairingCheckbox", formData, 'stereoPairing');
                    setCheckboxValue("#multiroomSupportCheckbox", formData, 'multiroomSupport');
                    setCheckboxValue("#waterproofCheckbox", formData, 'waterproof');
                    setCheckboxValue("#expandableStorageCheckbox", formData, 'expandableStorage');
                    setCheckboxValue("#displayScreenCheckbox", formData, 'displayScreen');
                    setCheckboxValue("#includeSoundbarCheckbox", formData, 'includeSoundbar');
                    setCheckboxValue("#includeReceiverCheckbox", formData, 'includeReceiver');
                    setCheckboxValue("#includeSubwooferCheckbox", formData, 'includeSubwoofer');
                    setCheckboxValue("#bluetoothSupportCheckbox", formData, 'bluetoothSupport');

                    function setFieldValue(fieldName, selectors) {
                        let fieldValue = null;
                        selectors.forEach(selector => {
                            const element = document.querySelector(selector);
                            if (element && element.offsetParent !== null && element.value) { // Check if the element is visible and has a value
                                fieldValue = element.value;
                            }
                        });

                        if (fieldValue !== null) {
                            formData.set(fieldName, fieldValue);
                        } else {
                            formData.delete(fieldName);
                        }
                    }

                    setFieldValue('processor', ['.desktop #processor', '.ultrabooks #processor', '.gamingLaptops #processor', '.standardLaptops #processor']);
                    setFieldValue('ram', ['.desktop #ram', '.ultrabooks #ram', '.gamingLaptops #ram', '.standardLaptops #ram']);
                    setFieldValue('displaySize', ['.ultrabooks #displaySize', '.gamingLaptops #displaySize', '.standardLaptops #displaySize', '.smartphones #displaySize', '.smartwatches #displaySize', '.tablets #displaySize']);
                    setFieldValue('storage', ['.ultrabooks #storage', '.gamingLaptops #storage', '.standardLaptops #storage', '.smartphones #storage']);
                    setFieldValue('graphicsCard', ['.desktop #graphicsCard', '.gamingLaptops #graphicsCard']);

                    Swal.fire({
                        title: 'Are you sure you want to add this product ?',
                        icon: 'warning',
                        showCancelButton: true,
                        confirmButtonColor: '#3085d6',
                        cancelButtonColor: '#d33',
                        confirmButtonText: 'Yes, add it !!'
                    }).then((result) => {
                        if (result.isConfirmed) {
                            $.ajax({
                                url: '<%= process.env.APP_URL_ADMIN %>/webAdmin/product/add-product',
                                method: form.method,
                                data: formData,
                                contentType: false,
                                processData: false,
                                success: function () {
                                    Swal.fire(
                                        'Added!',
                                        'Product added successfully',
                                        'success'
                                    ).then(() => {
                                        window.location.href = '<%= process.env.APP_URL_ADMIN %>/webAdmin/product/product-page/all';
                                    })
                                },
                                error: function (xhr, status, error) {
                                    const data = JSON.parse(xhr.responseText)
                                    Swal.fire({
                                        title: `${data.message || status}`,
                                        icon: 'error',
                                    })
                                }
                            });
                        }
                    }).catch(err => console.log(err))
                }
            })
        })


    </script>


    <%- include("../layouts/footer.ejs") %>