        function ImgUpload() {
            var imgArray = [];
            $(".upload__inputfile").change(function (e) {
                var imgWrap = $(this).closest(".upload__box").find(".upload__img-wrap");
                var maxLength = $(this).attr("data-max_length");
                var files = e.target.files;
                var formData = new FormData();
                for (var i = 0; i < files.length; i++) {
                    formData.append('image[]', files[i]);
                }

                // Display images in the frontend
                for (var i = 0; i < files.length; i++) {
                    var file = files[i];
                    if (!file.type.match('image.*')) {
                        continue;
                    }
                    if (imgArray.length < maxLength) {
                        imgArray.push(file);
                        var reader = new FileReader();
                        reader.onload = function (e) {
                            var html = "<div class='upload__img-box'><div style='background-image: url(" + e.target.result + ")' data-number='" + $(".upload__img-close").length + "' data-file='" + file.name + "' class='img-bg'><div class='upload__img-close'><button><img src='/img/delete.svg'></button></div></div></div>";
                            imgWrap.append(html);
                        };
                        reader.readAsDataURL(file);
                    }
                }

            });
        }

        function updateFormData(formData, field, value) {
            if (value !== undefined && value !== null && value !== '') {
